<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.wenyuan.myandroiddemo"
          xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    <uses-permission android:name="android.permission.CAMERA"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.wenyuan.myandroiddemo.MainActivity"
            android:launchMode="singleTop"
            android:theme="@style/AppTheme.Placeholder">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.wenyuan.myandroiddemo.encrypt.EncyptActivity"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name="com.wenyuan.myandroiddemo.hardware.camera.SysCameraActivity"
            android:configChanges="orientation|keyboardHidden|screenSize|locale|fontScale|layoutDirection"
            android:launchMode="singleTop"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.wenyuan.myandroiddemo.hardware.camera.CustomCameraActivity"
            android:configChanges="orientation|keyboardHidden|screenSize|locale|fontScale|layoutDirection"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.wenyuan.myandroiddemo.detail.permission.PermissionActivity">
        </activity>
        <activity android:name="com.wenyuan.myandroiddemo.detail.picture.HandlerPicActivity">
        </activity>
        <activity
            android:name="com.wenyuan.myandroiddemo.MoreActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_more"
            android:theme="@style/FullscreenTheme">
        </activity>
        <activity android:name="com.wenyuan.myandroiddemo.animation.PropertyAnimActivity">
        </activity>
        <activity android:name="com.wenyuan.myandroiddemo.animation.TweenAnimActivity">
        </activity>
        <activity android:name="com.wenyuan.myandroiddemo.animation.FrameAnimActivity">
        </activity>
        <activity android:name="com.wenyuan.myandroiddemo.animation.ActivityOptionsActivity">
        </activity>
        <activity android:name="com.wenyuan.myandroiddemo.layout.LayoutActivity">
        </activity>
        <activity android:name="com.wenyuan.myandroiddemo.storage.StorageActivity">
        </activity>
        <activity android:name="com.wenyuan.myandroiddemo.event.EventActivity">
        </activity>
        <activity android:name="com.wenyuan.myandroiddemo.customview.CustomViewActivity">
        </activity>
    </application>

</manifest>